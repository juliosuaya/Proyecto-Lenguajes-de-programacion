{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "linebreak-style": "off",
        "camelcase": "off",
        "no-console": "off",
        "no-use-before-define": "off",
        "linebreak-style": "off",
        "max-classes-per-file": "off",
    }
}